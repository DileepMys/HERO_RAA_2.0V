OPTIONAL_TOOL_DEPS := \
$(wildcard ../makefile.defs) \
$(wildcard ../makefile.init) \
$(wildcard ../makefile.targets) \


BUILD_ARTIFACT_NAME := imxrt1170_raaa_app
BUILD_ARTIFACT_EXTENSION := axf
BUILD_ARTIFACT_PREFIX :=
BUILD_ARTIFACT := $(BUILD_ARTIFACT_PREFIX)$(BUILD_ARTIFACT_NAME)$(if $(BUILD_ARTIFACT_EXTENSION),.$(BUILD_ARTIFACT_EXTENSION),)

# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all:
	+@$(MAKE) --no-print-directory main-build && $(MAKE) --no-print-directory post-build

# Main-build Target
main-build: imxrt1170_raaa_app.axf

# Tool invocations
imxrt1170_raaa_app.axf: $(OBJS) $(USER_OBJS) makefile objects.mk $(OPTIONAL_TOOL_DEPS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map="imxrt1170_raaa_app.map" -Xlinker --gc-sections -Xlinker -print-memory-usage -Xlinker --sort-section=alignment -Xlinker --cref -mcpu=cortex-m7 -mfpu=fpv5-d16 -mfloat-abi=hard -mthumb -T imxrt1170_raaa_app_Debug.ld -o "imxrt1170_raaa_app.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '

# Other Targets
clean:
	-$(RM) imxrt1170_raaa_app.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size "imxrt1170_raaa_app.axf" ; arm-none-eabi-objcopy -v -O ihex "imxrt1170_raaa_app.axf" "imxrt1170_raaa_app.hex" ; arm-none-eabi-objcopy -v -O binary "imxrt1170_raaa_app.axf" "imxrt1170_raaa_app.bin" ; arm-none-eabi-objcopy -v -O srec "imxrt1170_raaa_app.axf" "imxrt1170_raaa_app.s19"
	-@echo ' '

.PHONY: all clean dependents main-build post-build

-include ../makefile.targets
